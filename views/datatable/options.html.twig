{% if datatable.options.deferLoading is not same as(null) %}
    "deferLoading": {{ datatable.options.deferLoading }},
{% endif %}
{% if datatable.options.displayStart is not same as(null) %}
    "displayStart": {{ datatable.options.displayStart }},
{% endif %}
{% if datatable.options.dom is not same as(null) %}
    "dom": "{{ datatable.options.dom|e('js') }}",
{% endif %}
{% if datatable.options.lengthMenu is not same as(null) %}
    "lengthMenu": {{ datatable.options.lengthMenu|raw }},
{% endif %}
{% if datatable.options.order is not same as(null) %}
    "order": {{ datatable.options.order|raw }},
{% endif %}
{% if datatable.options.orderCellsTop is not same as(null) %}
    "orderCellsTop": {{ datatable.options.orderCellsTop|sg_datatables_bool_var }},
{% endif %}
{% if datatable.options.orderClasses is not same as(null) %}
    "orderClasses": {{ datatable.options.orderClasses|sg_datatables_bool_var }},
{% endif %}
{% if datatable.options.orderFixed is not same as(null) %}
    "orderFixed": {{ datatable.options.orderFixed|raw }},
{% endif %}
{% if datatable.options.orderMulti is not same as(null) %}
    "orderMulti": {{ datatable.options.orderMulti|sg_datatables_bool_var }},
{% endif %}
{% if datatable.options.pageLength is not same as(null) %}
    "pageLength": {{ datatable.options.pageLength }},
{% endif %}
{% if datatable.options.pagingType is not same as(null) %}
    "pagingType": "{{ datatable.options.pagingType }}",
{% endif %}
{% if datatable.options.renderer is not same as(null) %}
    "renderer": "{{ datatable.options.renderer }}",
{% endif %}
{% if datatable.options.retrieve is not same as(null) %}
    "retrieve": {{ datatable.options.retrieve|sg_datatables_bool_var }},
{% endif %}
{% if datatable.options.rowId is not same as(null) %}
    "rowId": "{{ datatable.options.rowId }}",
{% endif %}
{% if datatable.options.scrollCollapse is not same as(null) %}
    "scrollCollapse": {{ datatable.options.scrollCollapse|sg_datatables_bool_var }},
{% endif %}
{% if datatable.options.searchDelay is not same as(null) %}
    "searchDelay": {{ datatable.options.searchDelay }},
{% endif %}
{% if datatable.options.stateDuration is not same as(null) %}
    "stateDuration": {{ datatable.options.stateDuration }},
{% endif %}
{% if datatable.options.stripeClasses is not same as(null) %}
    "stripeClasses": {{ datatable.options.stripeClasses|raw }},
{% endif %}


